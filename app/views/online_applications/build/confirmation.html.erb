
<%= render :layout => 'form_application_group' do |f| %>

  <%= render 'confirmation', :f => f %>

  <div class="col-md-12 text-center">
    <h3><%=t :name_badge %></h3>
  </div>

  <div class="col-md-12">
    <%=t :badge_explanation %>
  </div>

  <%= f.fields_for :online_applications do |builder| %>
    <%= render 'name_badges', :f => builder, :online_application => builder.object %>
  <% end %>

<% end %>
